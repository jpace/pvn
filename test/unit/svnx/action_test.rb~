require 'rubygems'
require 'riel'

#!/usr/bin/ruby -w
# -*- ruby -*-

require 'tc'
require 'svnx/action'

module SVNx
  class ActionTestCase < SVNx::TestCase
    include Loggable
    
    def test_entry_from_xml
      doc = REXML::Document.new Resources::PT_LOG_R19_5.readlines.join('')
      entry = Entry.new :xmlelement => doc.elements[1].elements[4]
      assert_log_entry_16 entry
    end
  end
end
